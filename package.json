{
  "name": "taro-material",
  "private": true,
  "author": "lico",
  "version": "0.2.14",
  "description": "Mini Program components that implement Google's Material Design.",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "taro",
    "taro-ui"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "lint-staged": {
    "*.js": [
      "eslint --quiet --cache --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "scripts": {
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:doc": "npm run build:h5-static && node ./build/dev-server",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:component": "cross-env TARO_BUILD_TYPE=component taro build --ui",
    "build:h5-static": "npm run build:h5 && node ./build/build-static",
    "build:doc": "npm run build:h5-static && node ./build/build-doc",
    "prebuild": "rimraf packages/taro-material/dist/*",
    "build": "npm run build:component",
    "deploy": "npm run build && cp packages/taro-material/package.json dist/ && npm publish dist --tag latest",
    "lint": "eslint ./packages --fix",
    "lint:style": "stylelint \"packages/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"packages/**/*.scss\" --syntax scss --fix",
    "test": "jest --coverage",
    "test:ci": "npm run build:h5 && npm run test",
    "prettier": "yarn --silent prettier:files | xargs prettier --write",
    "prettier:files": "find . -name \"*.js\" -o -name \"*.d.ts\" -o -name \"*.tsx\" | grep -v -f .eslintignore",
    "prettier:check": "yarn --silent prettier:files | xargs prettier --list-different",
    "prettier:ci": "prettier --list-different"
  },
  "peerDependencies": {
    "taro": "*"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mapbox/jsxtreme-markdown-loader": "^0.8.3",
    "@tarojs/cli": "^1.1.9",
